{
  "name": "kirby-rocket",
  "version": "2.0.0",
  "description": "Kirby Rocket is a simple developer theme and boilerplate for the Kirby 3.5 CMS.",
  "author": "Manuel Deutsch",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mantsch/kirby-rocket.git"
  },
  "scripts": {
    "dev": "parcel watch ./assets/scss/main.scss ./assets/js/main.js --dist-dir ./assets/dist --public-url .",
    "build": "parcel build --no-content-hash ./assets/scss/main.scss ./assets/js/main.js --dist-dir ./assets/dist --public-url ."
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4",
    "@parcel/transformer-image": "^2.0.0",
    "@parcel/transformer-sass": "^2.0.0",
    "autoprefixer": "^10.2.6",
    "cssnano": "^5.0.6",
    "parcel": "^2.0.0",
    "postcss": "^8.2.4",
    "postcss-combine-media-query": "^1.0.1",
    "postcss-purgecss": "^1.0.0",
    "sass": "^1.32.5"
  },
  "dependencies": {
    "cookies-eu-banner": "^2.0.1",
    "normalize.css": "^8.0.1"
  },
  "prettier": {
    "printWidth": 120
  }
}
